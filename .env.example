# AWS Credentials - Set these from your secrets manager
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# AWS Configuration
AWS_REGION=us-east-2
S3_BUCKET=stockscompute

# OpenAI
OPENAI_API_KEY=your_openai_key_here
PREDICTION_AGENT_MODEL=gpt-4o-mini

# Amazon Nova (optional, only used when a Nova model is selected)
AMAZON_NOVA_API_KEY=your_amazon_nova_key_here
AMAZON_NOVA_API_ENDPOINT=https://your-nova-gateway.example.com/v1/chat/completions
AMAZON_NOVA_DEFAULT_MODEL=amazon.nova-lite-v1:0

# Global server-side model allowlist (comma-separated)
LLM_ALLOWED_MODELS=gpt-5-nano,gpt-5-mini,gpt-5,gpt-5.1,gpt-5.2

# Slack
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
SLACK_BOT_TOKEN=your_slack_bot_token_here
SLACK_CHANNEL=your_slack_channel_id_here

# Alpaca
