# AWS Credentials - Set these from your GitHub Codespaces secrets
# AWS_ACCESS_KEY_ID=your_access_key_id_here
# AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# AWS Configuration
AWS_REGION=us-east-2
S3_BUCKET=stockscompute

# OpenAI (required for the OpenAI ticker agent + newsletter enrichment)
OPENAI_API_KEY=your_openai_key_here
PREDICTION_AGENT_MODEL=gpt-4o-mini

# Slack (set one or both)
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
SLACK_BOT_TOKEN=your_slack_bot_token_here
SLACK_CHANNEL=your_slack_channel_id_here

# Alpaca (paper trading recommended by default)
ALPACA_API_BASE=https://paper-api.alpaca.markets
ALPACA_DATA_BASE=https://data.alpaca.markets
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_here

# Firebase Web Push (public VAPID key used by the browser token registration flow)
FCM_WEB_VAPID_KEY=your_public_web_push_key_here

# Gemini (server-side assistant in Firebase Functions)
GOOGLE_GENAI_API_KEY=your_gemini_api_key_here

# Social media automation (Firebase Functions)
# Cheap model default is gpt-4o-mini, can be changed with SOCIAL_CONTENT_MODEL.
SOCIAL_AUTOMATION_ENABLED=true
SOCIAL_CONTENT_MODEL=gpt-4o-mini
SOCIAL_AUTOMATION_TIMEZONE=America/New_York
SOCIAL_DISPATCH_BATCH_SIZE=30
SOCIAL_DEFAULT_CTA_URL=https://quantura-e2e3d.web.app

# Use one webhook per channel (direct API adapter/webhook bridge can be configured later)
SOCIAL_WEBHOOK_X=
SOCIAL_WEBHOOK_LINKEDIN=
SOCIAL_WEBHOOK_FACEBOOK=
SOCIAL_WEBHOOK_INSTAGRAM=
SOCIAL_WEBHOOK_THREADS=
SOCIAL_WEBHOOK_REDDIT=
SOCIAL_WEBHOOK_TIKTOK=
SOCIAL_WEBHOOK_YOUTUBE=
SOCIAL_WEBHOOK_PINTEREST=
